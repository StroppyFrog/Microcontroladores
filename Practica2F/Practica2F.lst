.................... int16 get_num() {
....................    int16 numero = 0;
....................    char cadena[5] = {0,0,0,0,0};
....................    int8 index = 0;
....................    char tecla;
....................    do {
....................       tecla = read_key();
....................       if((tecla >= 48) && (tecla <= 57)) 
....................       {
....................          cadena[index] = tecla;
....................          index++;
....................          printf(lcd_putc,"%c",tecla);
....................       }
....................       
....................       if(tecla == '=' && index > 0){
....................          break;     
....................       }
....................    }while(index < 5);
....................    
....................    for(int8 i = 0; i < index; i++){
....................       numero += (cadena[i] - 48) * (pow(10,index - 1 - i));
....................    }  
....................    return numero;
.................... }
